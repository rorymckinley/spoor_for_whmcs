{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "google",
        "plugin:vue/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "max-len": [2, 120, 2],
      "indent": ["error", 2, { "SwitchCase": 1 }],
      "no-unused-vars": ["error", {"varsIgnorePattern": "_"}]
    }
}
